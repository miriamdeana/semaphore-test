<div class='form-group'>
  <%= form.hidden_field :id, value: @id %>
</div>
<div class='form-group'>
  <%= form.label :name, class: 'col-sm-2 control-label' %>
  <div class='col-sm-10'>
    <%= form.text_field :name, readonly: true, class: 'form-control', value: @name %>
  </div>
</div>
<div class='form-group'>
  <%= form.label :email, class: 'col-sm-2 control-label' %>
  <div class='col-sm-10'>
    <%= form.text_field :email, readonly: true, class: 'form-control', value: @email %>
  </div>
</div>
<div class='form-group'>
  <%= form.label :phone, 'Calling From', class: 'col-sm-2 control-label' %>
  <div class='col-sm-10'>
    <%= form.text_field :phone, readonly: true, class: 'form-control', value: @number %>
  </div>
</div>